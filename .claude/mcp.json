{
  "mcpServers": {
    "trading-graph": {
      "command": "python",
      "args": ["/opt/data/trading_light_pilot/trader/graph/mcp_server.py"],
      "cwd": "/opt/data/trading_light_pilot/trader",
      "env": {
        "PYTHONPATH": "/opt/data/trading_light_pilot/trader",
        "NEO4J_URI": "bolt://localhost:7688",
        "NEO4J_USER": "neo4j"
      }
    },
    "trading-rag": {
      "command": "python",
      "args": ["/opt/data/trading_light_pilot/trader/rag/mcp_server.py"],
      "cwd": "/opt/data/trading_light_pilot/trader",
      "env": {
        "PYTHONPATH": "/opt/data/trading_light_pilot/trader",
        "DATABASE_URL": "postgresql://lightrag:lightrag@localhost:5433/lightrag",
        "OLLAMA_BASE_URL": "http://localhost:11434"
      }
    }
  }
}
